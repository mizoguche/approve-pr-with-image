{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "ava",
    "flowtype",
    "import",
    "jsx-a11y",
    "security"
  ],
  "parserOptions": {},
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "chrome": false
  },
  "rules": {
    "flowtype/define-flow-type": 1,
    "flowtype/use-flow-type": 1
  }
}
