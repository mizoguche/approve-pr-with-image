  },
  "homepage": "https://github.com/mizoguche/approve-pr-with-image",
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "failFast": false,
    "tap": false,
    "powerAssert": true,
    "require": [
      "babel-register"
