    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "ava",
    "lint": "eslint src test",
    "start": "webpack -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mizoguche/approve-pr-with-image.git"
  },
